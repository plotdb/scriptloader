doctype html
- var byjs = false
html
  mixin prejs(src)
    link(rel="preload",href=src,as="script")
  mixin script(src)
    script(src=src,type="text/javascript")
  head
    if byjs
      +prejs("assets/lib/bootstrap.native/main/bootstrap-native.min.js")
      +prejs("assets/lib/browserfs/main/browserfs.min.js")
      +prejs("js/fs.js")
      +prejs("assets/lib/@loadingio/ldquery/main/ldq.min.js")
      +prejs("assets/lib/@loadingio/debounce.js/main/debounce.min.js")
      +prejs("assets/lib/@plotdb/json0/main/json0.min.js")
      +prejs("assets/lib/@plotdb/datadom/main/datadom.min.js")
      +prejs("assets/lib/@plotdb/datahub/main/datahub.min.js")
      +prejs("assets/lib/@plotdb/pug-browser/main/pug.min.js")
      +prejs("assets/lib/@plotdb/rescope/main/rescope.min.js")
      +prejs("assets/lib/@plotdb/sandbox.js/main/sandbox.min.js")
      +prejs("assets/lib/ldview/main/ldview.min.js")
      +prejs("assets/lib/ldloader/main/ldld.min.js")
      +prejs("assets/lib/ldcover/main/ldcv.min.js")
      +prejs("js/async.js")
      +prejs("js/defer.js")
      +prejs("js/both.js")
      +prejs("js/block.js")

  body
    h1 hi
    if !byjs
      +script("assets/lib/bootstrap.native/main/bootstrap-native.min.js")
      +script("assets/lib/browserfs/main/browserfs.min.js")
      +script("js/fs.js")
      +script("assets/lib/@loadingio/ldquery/main/ldq.min.js")
      +script("assets/lib/@loadingio/debounce.js/main/debounce.min.js")
      +script("assets/lib/@plotdb/json0/main/json0.min.js")
      +script("assets/lib/@plotdb/datadom/main/datadom.min.js")
      +script("assets/lib/@plotdb/datahub/main/datahub.min.js")
      +script("assets/lib/@plotdb/pug-browser/main/pug.min.js")
      +script("assets/lib/@plotdb/rescope/main/rescope.min.js")
      +script("assets/lib/@plotdb/sandbox.js/main/sandbox.min.js")
      +script("assets/lib/ldview/main/ldview.min.js")
      +script("assets/lib/ldloader/main/ldld.min.js")
      +script("assets/lib/ldcover/main/ldcv.min.js")
      +script("js/async.js")
      +script("js/defer.js")
      +script("js/both.js")
      +script("js/block.js")
      +script("js/block.js")

    script(src="js/loader.js")
